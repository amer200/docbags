<%- include('../parts/head') %>
    <link rel="stylesheet" href="/css/index.css">
    <div class="head">
        <div class="container">
            <h1>DocBags 1.0</h1>
        </div>
    </div>
    <div class="main">
        <div class="container">
            <div class="row">
                <form action="/upload" method="post" enctype="multipart/form-data">
                    <div class="mb-3">
                        <h3> insert New Document</h3>
                    </div>
                    <div class="mb-3">
                        <label for="accountnum" class="form-label">Account Number</label>
                        <input type="text" class="form-control" id="accountnum" name="accountNum" required>
                    </div>
                    <div class="mb-3">
                        <label for="password" class="form-label">password</label>
                        <input type="password" class="form-control" id="password" name="password" required>
                    </div>
                    <div class="mb-3">
                        <label for="documenttype" class="form-label">Document type</label>
                        <input type="text" class="form-control" id="documenttype" name="docType" required>
                    </div>
                    <div class="mb-3">
                        <label for="date" class="form-label">item date</label>
                        <input type="date" class="form-control" id="date" name="itemDate" required>
                    </div>
                    <div class="mb-3">
                        <label for="type" class="form-label">item type</label>
                        <input type="text" class="form-control" id="type" required>
                    </div>
                    <div class="mb-3">
                        <input type="file" class="form-control" name="doc" id="file" required>
                    </div>
                    <div class="mb-3">
                        <button type="submit" class="btn btn-dark">submit</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <div class="footer">
        <div class="container">
            <div class="row">
                <ul>
                    <li><a href="#">uploading Document</a></li>
                    <li><a href="#">login & creating</a></li>
                    <li><a href="#">search or uploading page</a></li>
                    <li><a href="#">search Documents</a></li>
                </ul>
            </div>
        </div>
    </div>
    </body>

    </html>